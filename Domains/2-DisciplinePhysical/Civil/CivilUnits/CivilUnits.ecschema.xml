<ECSchema schemaName="CivilUnits" alias="cvu" version="01.00.00" description="This contains the unit definitions that are used across Civil domain-specific applications" xmlns="http://www.bentley.com/schemas/Bentley.ECXML.3.2">
    <ECSchemaReference name="CoreCustomAttributes" version="01.00.04" alias="CoreCA"/>
    <ECSchemaReference name="BisCustomAttributes" version="01.00.00" alias="bisCA"/>
    <ECSchemaReference name="Formats" version="01.00.00" alias="f"/>
    <ECSchemaReference name="Units" version="01.00.09" alias="u"/>

    <ECCustomAttributes>
        <ProductionStatus xmlns="CoreCustomAttributes.01.00.04">
            <SupportedUse>FieldTesting</SupportedUse>
        </ProductionStatus>
        <SchemaLayerInfo xmlns="BisCustomAttributes.01.00.00">
            <Value>DisciplinePhysical</Value>
        </SchemaLayerInfo>
    </ECCustomAttributes>

    <KindOfQuantity typeName="ANGLE" displayLabel="Civil Angle" description="Angular measurements for rotations, orientations, and directional calculations in civil engineering" persistenceUnit="u:RAD" presentationUnits="f:DefaultRealU(2)[u:ARC_DEG];f:AngleDMS" relativeError="0.0001"/>
    <KindOfQuantity typeName="AREA" displayLabel="Civil Area" description="Surface area measurements for land parcels, cross-sections, and hydraulic calculations" persistenceUnit="u:SQ_M" presentationUnits="f:DefaultRealU(2)[u:SQ_M];f:DefaultRealU(2)[u:SQ_FT]" relativeError="0.0001"/>
    <KindOfQuantity typeName="BEARING" displayLabel="Civil Bearing" description="Directional bearings for surveying, alignment layout, and navigation" persistenceUnit="u:RAD" presentationUnits="f:AngleDMS" relativeError="0.0001"/>
    <KindOfQuantity typeName="ELEVATION" displayLabel="Civil Elevation" description="Vertical height measurements relative to a datum for grading, profiles, and topographic surveys" persistenceUnit="u:M" presentationUnits="f:DefaultRealU(2)[u:M];f:DefaultRealU(2)[u:FT]" relativeError="0.0001"/>
    <KindOfQuantity typeName="FLOW" displayLabel="Civil Flow" description="Volumetric flow rate for water, stormwater, and wastewater hydraulic systems" persistenceUnit="u:CUB_M_PER_SEC" presentationUnits="f:DefaultRealU(2)[u:CUB_M_PER_SEC];f:DefaultRealU(2)[u:CUB_FT_PER_SEC]" relativeError="0.0001"/>
    <KindOfQuantity typeName="LENGTH" displayLabel="Civil Length" description="General linear distance measurements for alignments, dimensions, cross-sections, and spatial layouts" persistenceUnit="u:M" presentationUnits="f:DefaultRealU(2)[u:M];f:DefaultRealU(2)[u:FT];f:DefaultRealU(2)[u:US_SURVEY_FT]" relativeError="0.0001"/>
    <KindOfQuantity typeName="LENGTH_SHORT" displayLabel="Civil Short Length" description="Short-scale linear measurements for pipe diameters, material thicknesses, and detailed dimensions" persistenceUnit="u:M" presentationUnits="f:DefaultRealU(1)[u:MM];f:DefaultRealU(1)[u:IN]" relativeError="0.0001"/>
    <KindOfQuantity typeName="SLOPE" displayLabel="Civil Slope" description="Gradient measurements for roadways, channels, pipes, cross-sections, and terrain slopes" persistenceUnit="u:M_PER_M" presentationUnits="f:DefaultRealU(2)[u:PERCENT_SLOPE]" relativeError="0.0001" />
    <KindOfQuantity typeName="STATION" displayLabel="Civil Station" description="Linear referencing measurements along alignments for roads, railways, and linear infrastructure" persistenceUnit="u:M" presentationUnits="f:StationZ_1000_3[u:M];f:StationZ_100_2[u:FT];f:StationZ_100_2[u:US_SURVEY_FT];f:DefaultRealU(2)[u:M];f:DefaultRealU(2)[u:FT];f:DefaultRealU(2)[u:US_SURVEY_FT]" relativeError="0.0001"/>
    <KindOfQuantity typeName="VELOCITY" displayLabel="Civil Velocity" description="Speed measurements for vehicles, water flow, and hydraulic velocity calculations" persistenceUnit="u:M_PER_SEC" presentationUnits="f:DefaultRealU(2)[u:KM_PER_HR];f:DefaultRealU(2)[u:MPH]" relativeError="0.0001"/>
    <KindOfQuantity typeName="VOLUME" displayLabel="Civil Volume" description="Volumetric measurements for earthwork, storage facilities, and material quantities" persistenceUnit="u:CUB_M" presentationUnits="f:DefaultRealU(2)[u:CUB_M];f:DefaultRealU(2)[u:CUB_FT]" relativeError="0.0001"/>
    <KindOfQuantity typeName="PERCENTAGE" displayLabel="Civil Percentage" description="Percentage values for proportions, grades, and statistical calculations" persistenceUnit="u:DECIMAL_PERCENT" presentationUnits="f:DefaultRealU(2)[u:PERCENT];f:DefaultRealU(4)[u:DECIMAL_PERCENT]" relativeError="0.0001"/>
    <KindOfQuantity typeName="SIDE_SLOPE" displayLabel="Civil Side Slope" description="Horizontal-to-vertical slope ratios for embankments, cuts, and channel side slopes" persistenceUnit="u:M_HORIZONTAL_PER_M_VERTICAL" presentationUnits="f:DefaultRealU(1)[u:HORIZONTAL_PER_VERTICAL]" relativeError="0.0001"/>
    <KindOfQuantity typeName="INTENSITY_INFILTRATION" displayLabel="Civil Intensity/Infiltration" description="Rainfall intensity and soil infiltration rate measurements for hydrologic and drainage design" persistenceUnit="u:M_PER_SEC" presentationUnits="f:DefaultRealU(2)[u:MM_PER_HR];f:DefaultRealU(2)[u:IN_PER_HR]" relativeError="0.0001"/>
    <KindOfQuantity typeName="RATIONAL_COEFFICIENT" displayLabel="Civil Rational Coefficient" description="Runoff coefficient for the Rational Method in stormwater drainage calculations" persistenceUnit="u:COEFFICIENT" presentationUnits="f:DefaultRealU(2)[u:COEFFICIENT]" relativeError="0.0001"/>
    <KindOfQuantity typeName="MANNINGS_N" displayLabel="Civil Manning's n" description="Manning's roughness coefficient for open channel and pipe flow calculations" persistenceUnit="u:COEFFICIENT" presentationUnits="f:DefaultRealU(3)[u:COEFFICIENT]" relativeError="0.0001"/>
    <KindOfQuantity typeName="HAZEN_WILLIAMS_C" displayLabel="Civil Hazen-William's C" description="Hazen-Williams C coefficient for pressurized pipe flow and water distribution system calculations" persistenceUnit="u:COEFFICIENT" presentationUnits="f:DefaultRealU(0)[u:COEFFICIENT]" relativeError="0.0001"/>
    <KindOfQuantity typeName="VOLUME_PER_LENGTH" displayLabel="Civil Volume per Length" description="Volume per unit length for earthwork cross-sections, prism calculations, and material quantities along alignments" persistenceUnit="u:CUB_M_PER_M" presentationUnits="f:DefaultRealU(2)[u:CUB_M_PER_M];f:DefaultRealU(2)[u:CUB_FT_PER_FT]" relativeError="0.0001"/>
    
</ECSchema>
