<?xml version="1.0" encoding="UTF-8"?>
<ECSchema schemaName="Terrain" alias="trrn" version="01.00.00" xmlns="http://www.bentley.com/schemas/Bentley.ECXML.3.1">
    <ECSchemaReference name="BisCore" version="01.00.04" alias="bis" />

    <ECCustomAttributes>
        <ProductionStatus xmlns="CoreCustomAttributes.1.0">
            <SupportedUse>NotForProduction</SupportedUse>
            <Checksum>0</Checksum>
        </ProductionStatus>
    </ECCustomAttributes>

    <ECEntityClass typeName="TerrainReference" displayLabel="Terrain Reference" description="A reference to a service providing the base terrain for a portion of the earth.">
        <BaseClass>bis:SpatialLocationElement</BaseClass>
        <ECProperty propertyName="ServiceName"  typeName="string"                           displayLabel="Service Name"  description="The name of the service that provides that terrain." />
        <ECProperty propertyName="ServiceUrl"   typeName="string"                           displayLabel="Service URL"   description="The URL of the service that provides the terrain." />
        <ECProperty propertyName="Priority"     typeName="int"                              displayLabel="Priority"      description="The relative priority of this TerrainReference to other TerrainReferences. Where TerrainReferences overlap, the one with the larger Priority is used."  />
     </ECEntityClass>

</ECSchema>
