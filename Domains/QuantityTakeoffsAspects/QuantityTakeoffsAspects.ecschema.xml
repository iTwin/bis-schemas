<ECSchema schemaName="QuantityTakeoffsAspects" alias="spcomp" version="01.00.00" xmlns="http://www.bentley.com/schemas/Bentley.ECXML.3.1">
    <ECSchemaReference name="BisCore" version="01.00.02" alias="bis" />
